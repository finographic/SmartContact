        document.addEventListener("DOMContentLoaded", function () {

 

    // Configure the three ways that SocketIOFileUpload can read files:
   // document.getElementById("upload_btn").addEventListener("click", siofu.prompt, false);


    siofu.addEventListener("load", function (event) {
        console.log('file has been loaded');
        $('#upload_status_div').html('<span>File has been uploaded</span>');
    });

    siofu.addEventListener("start", function (event) {

        console.log(nav_edt_current);
        if (!$('.editmodal').is(':visible'))
        {
            event.file.meta.business_unit = sipconfigdb.business_unit_wk;
            event.file.meta.skill_wk = user_new_camp_obj.skill_wk;
            event.file.meta.user_wk = sipconfigdb.online_user_wk;
        } else {




            event.file.meta.business_unit = nav_edt_current[3];
            event.file.meta.skill_wk =  nav_edt_current[2];
            event.file.meta.user_wk = sipconfigdb.online_user_wk;
        }
        $('#upload_btn_div').toggle();
        $('#upload_div').show();
    });

    // Do something on upload progress:
    siofu.addEventListener("progress", function (event) {
        $('#upload_bar').show();
        var percent = event.bytesLoaded / event.file.size * 100;
        console.log("File is", percent.toFixed(0), "percent loaded");
        //$('#upload_btn_div').toggle();
        //$('#upload_div').toggle();
        console.log(percent.toFixed(0) + '%');
        if (percent.toFixed(0) % 20 == 0){
        $('#upload_progress_div').attr('aria-valuenow', percent.toFixed(0)).css('width', percent.toFixed(0) + '%');
        }
        $('#upload_bar').val(percent.toFixed(0));
        // $('#upload_bar').css('width', percent.toFixed(0) + '%');
        $('#upload_status_div').html("<span>File is " + percent.toFixed(0).toString() + "% loaded<span>");
    });
    // Do something when a file is uploaded:
    siofu.addEventListener("error", function (event) {
        $('#upload_status_div').html('<span class="text-danger">Error Occured on File Upload, please try again.</span><BR><BR>');
           $('#upload_btn_div').toggle();
           $('#upload_bar').toggle();
    });

    siofu.addEventListener("complete", function (event) {
            console.log(event.success);
            $('#upload_status_div').html('<span>File upload completed</span>');
            $('#upload_status_div').html('<span>File is being processed</span><BR><BR> <i class="fa fa-3x fa-spinner fa-spin" aria-hidden="true"></i>');
            $('#upload_bar').toggle();
        //$('#upload_btn_div').toggle();
        //$('#upload_div').toggle();
    });


    }, false);

</script>


