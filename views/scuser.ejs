        function sumbiz(arrin, prop, bizu, typesum) {
            var total = 0;
            for (var i = 0, _len = arrin.length; i < _len; i++) {
         
                if (arrin[i][prop] == bizu && arrin[i]['reptype'] == typesum )
                {
                    total += parseInt(arrin[i]['distinct_rec_count']);
                  
                }
            }
            return total
        }



        function sum_skill(arrin, prop, bizu, typesum) {
            var total = 0;
            for (var i = 0, _len = arrin.length; i < _len; i++) {

                if (arrin[i][prop] == bizu && arrin[i]['reptype'] == typesum) {
                    total += parseInt(arrin[i]['sum_call']);
                    console.log(parseInt(arrin[i]['sum_call']));
                }
            }
            return total
        }



        function sumbiz_att(arrin, prop, bizu, typesum, att_in) {
            var total = 0


            for (var i = 0, _len = arrin.length; i < _len; i++) {

                if (arrin[i][prop] == bizu && arrin[i]['reptype'] == typesum && (arrin[i].attempt_log_attempts == att_in)) {
                    total += parseInt(arrin[i]['distinct_rec_count']);

                }
            }
            return total
        }



