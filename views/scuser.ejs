        function appt_specf_formatter_st(value, row, index) {
            return moment(value.substring(0, 4), ["HHmm"]).format("h:mm A");
        };

        function appt_specf_formatter_end(value, row, index) {
            return moment(value.substring(4, 8), ["HHmm"]).format("h:mm A");
        };

        function appt_specf_formatter_ex(value, row, index) {

            if (value.substring(9, 13) + value.substring(13, 17) == '00000000') {
                return '';
            }else{
                return moment(value.substring(9, 13), ["HHmm"]).format("h:mm A") + ' - ' + moment(value.substring(13, 17), ["HHmm"]).format("h:mm A");
            }
        };

        function appt_specf_formatter_min(value, row, index) {
            return value + ' minutes';
        };

        function appt_specf_formatter_dt(value, row, index) {

         

            return (value == null) ? '' : moment(value, ["YYYY-MM-DD"]).format("M-DD-YYYY");;
        };

